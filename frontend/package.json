{
  "name": "portfolio-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "node scripts/local-https.js",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "start": "next start",
    "start:standalone": "node .next/standalone/server.js",
    "start:prod": "NODE_ENV=production node .next/standalone/server.js",
    "start:https": "NODE_ENV=production node scripts/local-https.js",
    "lint": "next lint",
    "lint:unused": "eslint . --ext .js,.jsx,.ts,.tsx --fix --max-warnings=0",
    "css:purge": "npx purgecss --css app/globals.css,app/styles/**/*.css --content app/**/*.{js,jsx,ts,tsx} --output purged",
    "css:find-unused": "npx purgecss --css app/globals.css,app/styles/**/*.css --content app/**/*.{js,jsx,ts,tsx} --rejected",
    "cleanup": "npm run lint:unused && npm run css:find-unused",
    "production": "npm run build:prod && npm run start:standalone"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "canvas": "^3.1.0",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "openai": "^4.103.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-modal-video": "^2.0.1",
    "react-pdf": "^7.5.1",
    "react-slick": "^0.29.0",
    "sass": "^1.69.5",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/react-slick": "^0.23.12",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-unused-imports": "^4.1.4",
    "postcss": "^8.4.31",
    "purgecss": "^7.0.2",
    "typescript": "^5.3.2"
  }
}
